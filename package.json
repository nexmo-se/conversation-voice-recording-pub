{
  "name": "conversation-voice-recording",
  "version": "0.1.0",
  "private": true,
  "description": "Start/Stop/Pause/Resume Conversation Recording using Conversation and Voice API",
  "author": "Kitt Phi <Kitt.Phi@Vonage.com>",
  "proxy": "http://localhost:8054/",
  "secure": false,
  "main": "server.js",
  "engines": {
    "npm": "8.5.5",
    "node": "16.14.2"
  },
  "dependencies": {
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@vonage/server-sdk": "^2.10.7-beta-2",
    "axios": "^0.24.0",
    "concurrently": "^6.4.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "pusher": "^5.0.0",
    "pusher-js": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-phone-input-2": "^2.14.0",
    "react-scripts": "4.0.3",
    "uuid": "^8.3.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "deploy": "git add . && git commit -m update-repo && git push && npm run logs:prod",
    "logs:prod": "heroku logs --tail --app conv-voice-record",
    "heroku-postbuild": "npm install --only=dev --no-shrinkwrap && npm run build",
    "server": "nodemon server.js",
    "client": "react-scripts start",
    "dev": "concurrently -k \"npm run server\" \"npm run client\"",
    "prod": "concurrently -k \"npm run server\" \"npm run client\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nexmo-se/conversation-voice-recording.git"
  },
  "keywords": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nexmo-se/conversation-voice-recording/issues"
  },
  "homepage": "https://github.com/nexmo-se/conversation-voice-recording#readme",
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
}
